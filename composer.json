{
    "name": "tzurbaev/laravel-product-fields",
    "description": "Product fields support for Laravel models",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tzurbaev",
            "email": "zurbaev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": "~5.4.0|~5.5.0",
        "illuminate/support": "~5.4.0|~5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "orchestra/testbench": "^3.4|~3.5"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\ProductFields\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "database/"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\ProductFields\\ProductFieldsServiceProvider"
            ]
        }
    }
}
